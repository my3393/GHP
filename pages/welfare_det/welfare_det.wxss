/* pages/welfare_det/welfare_det.wxss */
@import '../store_detail/store_detail';

page {
  background: #f9f9f9;
}

.bg-white {
  background: #fff;
}
.back{
  background: #fff;
  margin-left: 0;
}
.top_dao {
  position: relative;
  position: absolute;
  bottom: 18rpx;
  left: 10rpx;
  width: 160rpx;
  height: 62rpx;
  border-radius: 40rpx;
  background: rgba(0, 0, 0, 0.3);
}

.top_dao image {
  width: 32rpx;
  height: 32rpx;
}

.b_back {
  position: absolute;
  left: 20rpx;
  top: 16rpx;
}

.daoh {
  position: absolute;
  right: 30rpx;
  top: 16rpx;
}

.top_img {
  width: 100%;
  height: 370rpx;
  background-size: cover;
}

.top_center>text {
  margin-right: 20rpx;
  display: inline-block;
  font-size: 30rpx;
}

.center_a {
  color: #f12200;
  border-bottom: 2rpx solid #f12200;
}

.banner_top {
  position: relative;
}

.banner {
  width: 100%;
  height: 1000rpx;
}

.banner image {
  width: 100%;
  height: 100%;
}

.banner_num {
  position: absolute;
  bottom: 55rpx;
  right: 20rpx;
  width: 92rpx;
  font-size: 22rpx;
  height: 42rpx;
  background: rgba(0, 0, 0, 0.3);
  color: #fff;
  border-radius: 21rpx;
  text-align: center;
  line-height: 42rpx;
}

.nav {
  background: #fff;
  position: relative;
  top: -50rpx;
  width: calc(100% - 40rpx);
  border-radius: 14rpx;
  margin-left: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
}

.g-h1 {
  width: 550rpx;
  margin-top: 20rpx;
  font-size: 30rpx;
  margin-left: 10rpx;
}

.g-h2 {
  color: #999;
  font-size: 24rpx;
  margin-left: 10rpx;
  margin-top: 10rpx;
}

.jindu {
  margin-left: 10rpx;
}

.g-bott {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}

.g-bo {
  text-align: right;
}

.g-bott-j {
  color: #333;
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
}

.g-boot-y {
  color: #999;
  font-size: 24rpx;
}

progress {
  color: #f12200;
}

.title {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  margin-top: 20rpx;
}

.title_h1 {
  font-weight: 600;
}

.title_tab {
  font-size: 28rpx;
  color: #333;
  margin-top: 30rpx;
  line-height: 40rpx;
}

.title_all {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  height: 60rpx;
  box-shadow: 2rpx -10rpx 4rpx #999;
  border-radius: 8rpx;
  line-height: 60rpx;
  background: #fff;
}

.title_all image {
  width: 18rpx;
  height: 10rpx;
}

.duo {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  overflow: hidden;
}

.t_kong {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #999;
  margin: 60rpx 0;
}

.t_kong image {
  width: 290rpx;
  height: 250rpx;
}

.neir {
  padding: 20rpx;
  background: #f5f5f5;
  box-sizing: border-box;
  font-size: 24rpx;
  margin: 20rpx 0;
}

.top {
  margin-top: 50rpx;
}

.top>view {
  width: 30%;
  display: inline-block;
  height: 257rpx;
  text-align: center;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  border-top: 6rpx solid #f12200;
  overflow: hidden;
  position: relative;
  font-size: 24rpx;
}

.top>view:nth-child(3) {
  border-right: 0;
  border-top-left-radius: 0;
  border-left: 1rpx solid #ccc;
}

.top>view:nth-child(2) {
  width: 35%;
  height: 303rpx;
}

.top>view:nth-child(1) {
  border-top-right-radius: 0;
  border-right: 1rpx solid #ccc;
}

.top_img {
  width: 59rpx;
  height: 38rpx;
  position: absolute;
  top: -33rpx;
  left: 20rpx;
}

.top_img image {
  width: 100%;
  height: 100%;
}

.top_tx {
  width: 82rpx;
  height: 82rpx;
  position: relative;
  display: inline-block;
  margin-top: 40rpx;
  border-radius: 50%;
  border: 6rpx solid #cdd9e7;
}

.top_tx image {
  width: 100%;
  height: 100%;
}

.tp_n {
  margin: 10rpx 0;
}

.top_list {
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #f3f3f3;
  font-size: 24rpx;
}

.top_list_img {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
  margin-right: 14rpx;
  margin-top: 15rpx;
}

.top_list_img image {
  width: 100%;
  height: 100%;
}

.top>view:nth-child(2)>.top_tx {
  border: 6rpx solid #ffca4a;
}

.top>view:nth-child(3)>.top_tx {
  border: 6rpx solid #f2a871;
}

.top_list_name {
  display: inline-block;
}

.top_list_date {
  float: right;
  color: #ccc;
}

.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  background: #fff;
  display: flex;
  justify-content: space-around;
}

.footer>view {
  margin-top: 10rpx;
}

.footer>view:nth-child(1) {
  text-align: center;
  font-size: 22rpx;
  margin-top: 20rpx;
}

.fenx {
  width: 34rpx;
  height: 34rpx;
  display: inline-block;
}

.fenx image {
  width: 100%;
  height: 100%;
}

.submit {
  width: 82%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  border-radius: 40rpx;
  background: linear-gradient(to right, #f12200, #ff4e17);
}
