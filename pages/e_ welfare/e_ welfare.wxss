/* pages/e_ welfare/e_ welfare.wxss */
page{
    font-size: 26rpx;
    background: #f9f9f9;
    font-family: PingFang-SC-Medium;
}
.banner{
    width: 100%;
    height: 430rpx;
}
.banner image{
    width: 100%;
    height: 100%;
}
.progre{
   position: relative;
   top: -20rpx;
    width: calc(100% - 40rpx);
    margin-left: 20rpx;
    background: #fff;
    padding: 20rpx;
    box-sizing: border-box;
    border-radius: 8rpx;
   
}
.pro-t{
    display: flex;
    justify-content: space-around;
}
.progress{
  color: #f12200;
}
.money{
    display: inline-block;
    width: 28rpx;
    height: 28rpx;
    margin-right: 6rpx;
    color: #666666;
    margin-top: 50rpx;
}
.money image{
    width: 100%;
    height: 100%;
    vertical-align: middle;
}
.money-s{
    font-size: 22rpx;
    margin-left: 8rpx;
}
.money-m{
    vertical-align: middle;
    color: #333333;
    display: inline-block;
    font-weight: 550;
    
}
.jiao{
    width: calc(100% - 40rpx);
    margin-left: 20rpx;
    height: 80rpx;
    text-align: center;
    background: #F22702;
    line-height: 80rpx;
    margin-top: 50rpx;
    border-radius: 60rpx;
    color: #fff;
}
.project{
    width: 100%;
    padding: 20rpx;
    box-sizing: border-box;
}
.h2{
    color: #000;
    font-size: 40rpx;
    display: inline-block;
    font-weight: 550;
    font-family: PingFang-SC-Bold;
}
.pro-r{
    float: right;
    margin-top: 8rpx;
}
.pro-n{
    vertical-align: middle;
    display: inline-block;
}
.pro-r-img{
    width: 24rpx;
    height: 24rpx;
    display: inline-block;
}
.pro-r-img image{
    width: 100%;
    height: 100%;
    vertical-align: middle;
}
.tag{
    padding: 6rpx 20rpx;
    box-sizing: border-box;
    display: inline-block;
    border-radius: 24rpx;
    background: #f3f3f3;
    margin-right: 30rpx;
    margin-top: 30rpx;
    
}
.active{
    background: #F22502;
    color: #fff;
}
.nav{
    background: #fff;
    margin-top: 30rpx;
}
.g-banner{
    width: 100%;
    height: 400rpx;
}
.g-banner image{
    width: 100%;
    height: 100%;
}
.g-h1{
    width: 550rpx;
    margin-top: 20rpx;
    font-size: 30rpx;
    margin-left: 10rpx;
}
.g-h2{
    color: #999;
    font-size: 24rpx;
    margin-left: 10rpx;
    margin-top: 10rpx;
}
.jindu{
   
    margin-left: 10rpx;
}
.g-bott{
    display: flex;
    justify-content: space-between;
    margin-top: 20rpx;
}
.g-bo{
    text-align: right;
}
.g-bott-j{
    color: #333333;
    font-size: 36rpx;
    font-family: PingFang-SC-Bold;
}
.g-boot-y{
    color: #999999;
    font-size: 24rpx;
}
/* canvas */
/* pages/canvasDemo/canvasDemo.wxss */
.canvasBox{
  height: 240rpx;
  position: relative;
  width: 240rpx;
  /* background-color: #f5f5f5; */
}
.bigCircle{
  width: 204rpx;
  height: 204rpx;
  border-radius: 50%;
  position: absolute;
  top:2rpx;
  bottom: 0;
  left:-15rpx;
  right: 0;
  margin: auto auto;
  background-color: #ddd;
}
.littleCircle{
  width: 178rpx;
  height: 178rpx;
  border-radius: 50%;
  position: absolute;
  top:0rpx;
  bottom: 0;
  left:-15rpx;
  right: 0;
  margin: auto auto;
  background-color: #eee;
}
.canvas{
  width: 215rpx;
  height: 215rpx;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto auto;
  z-index: 99;
}
.appl{
  position: fixed;
  right: 20rpx;
  bottom: 60rpx;
  width: 90rpx;
  height: 90rpx;
}
.appl image{
  width: 100%;
  height: 100%;
}